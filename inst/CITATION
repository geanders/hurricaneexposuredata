year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{hurricaneexposuredata}: Data Characterizing Exposure to Hurricanes in United States Counties",
         author = c(person("Brooke", "Anderson"),
                    person("Andrea", "Schumacher"),
                    person("William", "Crosson"),
                    person("Mohammad", "Al-Hamdan"),
                    person("Meilin", "Yan"),
                    person("Joshua", "Ferreri"),
                    person("Ziyu", "Chen"),
                    person("Steven", "Quiring"),
                    person("Seth", "Guikema")),
         year = year,
         note = note,
         url = "https://github.com/geanders/hurricaneexposuredata")
